            </sequence>
          </extension>
        </complexContent>
      </complexType>
      <simpleType name="LogoFlg">
        <restriction base="xsd:string">
          <enumeration value="FALSE"/>
          <enumeration value="TRUE"/>
        </restriction>
      </simpleType>
      <simpleType name="ThumbnailFlg">
        <restriction base="xsd:string">
          <enumeration value="FALSE"/>
          <enumeration value="TRUE"/>
        </restriction>
      </simpleType>
      <simpleType name="UserStatus">
        <restriction base="xsd:string">
          <enumeration value="ACTIVE"/>
          <enumeration value="PAUSED"/>
        </restriction>
      </simpleType>
      <simpleType name="MediaFileType">
        <restriction base="xsd:string">
          <enumeration value="JPEG"/>
          <enumeration value="GIF"/>
          <enumeration value="PNG"/>
        </restriction>
      </simpleType>
      <simpleType name="MediaType">
        <restriction base="xsd:string">
          <enumeration value="IMAGE"/>
          <enumeration value="ANIMATION_IMAGE"/>
        </restriction>
      </simpleType>
      <simpleType name="MediaApprovalStatus">
        <restriction base="xsd:string">
          <enumeration value="APPROVED"/>
          <enumeration value="REVIEW"/>
          <enumeration value="PRE_DISAPPROVED"/>
          <enumeration value="POST_DISAPPROVED"/>
        </restriction>
      </simpleType>
      <simpleType name="CampaignBannerFlg">
        <restriction base="xsd:string">
          <enumeration value="FALSE"/>
          <enumeration value="TRUE"/>
        </restriction>
      </simpleType>
      <complexType name="MediaOperation">
        <complexContent>
          <extension base="tns:Operation">
            <sequence>
              <element maxOccurs="1" minOccurs="1" name="accountId" type="xsd:long"/>
